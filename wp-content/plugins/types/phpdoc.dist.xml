<?xml version="1.0" encoding="UTF-8" ?>
<phpdoc>
    <parser>
        <target>docs/phpdoc</target>
    </parser>
    <transformer>
        <target>docs/phpdoc</target>
    </transformer>
    <files>
        <directory>.</directory>
        <ignore>vendor/*</ignore>
        <ignore>Twig/*</ignore>
        <ignore>composer/*</ignore>
        <ignore>library/otgs/*</ignore>
        <ignore>library/toolset/types/*</ignore>
    </files>
</phpdoc>